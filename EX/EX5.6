//Modify the two test( ) methods in the previous two
//programs so that they take two extra arguments, begin and end, and so that
//testval is tested to see if it is within the range between (and including) begin
//and end.

public class Exercise {
    static int result = 0;
    static void test1(int testval, int begin, int end) {
        if(testval >= begin && testval <= end )
            result = +1;
        else
            result = -1;
    }

    static int test2(int testval, int begin, int end){
        if(testval >= begin && testval <= end )
            return 1;
        return 0;
    }

    public static void main(String[] args) {
            test1(420,10,15);
        System.out.println(result);
            test1(420, -100, 1000);
        System.out.println(result);
        System.out.println(test2(420,10,15));
        System.out.println(test2(420,-100, 1000));
    }
}
