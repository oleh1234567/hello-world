public class DataOnly {
    int i;
    double d;
    boolean b;
public static void main(String[] args){
    DataOnly data = new DataOnly();
    data.i = 47;
    data.b = false;
    data.d = 1.1;
    System.out.println("i = " + data.i);
    System.out.println("d = " + data.d);
    System.out.println("b = " + data.b);
}
}
